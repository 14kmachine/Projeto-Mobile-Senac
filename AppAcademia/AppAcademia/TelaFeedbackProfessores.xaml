<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="AppAcademia.TelaFeedbackProfessores">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Feedback dos Professores"
                   HorizontalOptions="Center"
                   Margin="10,30,10,10"/>

            <Label Text="Digite seu feedback aqui '-'"
                   Margin="10"/>

            <Button Text="Enviar"
                    x:Name="buttonEnviarFeedback"
                    HorizontalOptions="FillAndExpand"
                     VerticalOptions="EndAndExpand"/>

            <!--<Button Text="Voltar"
                    x:Name="buttonVoltar"
                     Margin="50,0"/>-->


        </StackLayout>
    </ContentPage.Content>
</ContentPage>